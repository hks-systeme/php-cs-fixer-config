{
  "name": "hks-systeme/php-cs-fixer-config",
  "description": "Provides a configuration factory and multiple rule sets for friendsofphp/php-cs-fixer.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/hks-systeme/php-cs-fixer-config",
  "support": {
    "issues": "https://github.com/hks-systeme/php-cs-fixer-config/issues",
    "source": "https://github.com/hks-systeme/php-cs-fixer-config"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "friendsofphp/php-cs-fixer": "~3.57.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42.0",
    "ergebnis/license": "^2.4.0",
    "ergebnis/test-util": "^1.6.0",
    "phpunit/phpunit": "^9.6.19",
    "psalm/plugin-phpunit": "~0.19.0",
    "vimeo/psalm": "^5.23"
  },
  "autoload": {
    "psr-4": {
      "HKS\\PhpCsFixer\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HKS\\PhpCsFixer\\Config\\Test\\": "test"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "7.4.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
